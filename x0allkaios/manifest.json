{
  "manifest_version": 2,
  "name": "X0 All KaiOS",
  "short_name": "X0 All",
  "description": "A comprehensive app store for KaiOS applications.",
  "start_url": "./",
  "scope": "./",
  "display": "standalone",
  "orientation": "portrait",
  "icons": [
    {
      "src": "/favicon.ico",
      "sizes": "192x192",
      "type": "image/x-icon"
    },
    {
      "src": "/favicon.ico",
      "sizes": "512x512",
      "type": "image/x-icon"
    },
    {
      "src": "/favicon.ico",
      "sizes": "144x144",
      "type": "image/x-icon"
    },
    {
      "src": "/favicon.ico",
      "sizes": "96x96",
      "type": "image/x-icon"
    }
  ],
  "default_locale": "en",
  "theme_color": "#FFFFFF",
  "background_color": "#FFFFFF"
}
